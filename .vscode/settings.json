{
   "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true,
      ".vscode": true,
      ".next": true,
      "node_modules": true,
   },
   "explorer.fileNesting.enabled": true,
   "explorer.fileNesting.patterns": {
      "*.ts": "${capture}.js",
      "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
      "*.jsx": "${capture}.js",
      "*.tsx": "${capture}.ts",
      "tsconfig.json": "tsconfig.*.json",
      "package.json": ".gitignore, package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock, .eslint*, .prettierrc, *.mjs, *.ts, *.md, *.json"
   }
}